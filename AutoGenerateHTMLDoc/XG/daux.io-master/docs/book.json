{
    "title": "腾讯移动推送 文档",
    "description": "腾讯移动推送相关文档记录",
    "language": "zh-hans",
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "splitter@^0.0.8",
        "-sharing",
        "simple-page-toc@^0.1.1",
        "anchor-navigation-ex@0.1.8"
    ],
    "pluginsConfig": {
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "anchor-navigation-ex": {
            "isShowTocTitleIcon": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        }
    },
    "links": {
      "sidebar": {
        "腾讯移动推送": "http://xg.qq.com/"
      }
    }
}
